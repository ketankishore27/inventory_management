def get_stock_devices():

    sql_string = """
    select count(distinct(service_tag_number)) from inventory where lower(status) like '%stock%'
    """

    counts = pd.read_sql(text(sql_string), engine)
    return counts.to_dict("records")[0]


get_stock_devices()





